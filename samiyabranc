<script>
    jQuery(document).ready(function() {

        $('[data-toggle="tooltip"]').tooltip();
        $('.lazy').Lazy({
            // your configuration goes here
            scrollDirection: 'vertical',
            effect: 'show',
            visibleOnly: true,
            onError: function(element) {
                console.log('error loading ' + element.data('src'));
            }
        });
        var windowWidth = window.innerWidth;
        if (windowWidth < 768) {
            var s = jQuery("#menu");
            var r = jQuery("#menu-sidebar, .mobile-margin");
            var pos = s.position();
            if (pos && pos != undefined) {
                jQuery(window).scroll(function() {
                    var windowpos = jQuery(window).scrollTop();
                    if (windowpos >= pos.top & windowpos >= 10) {
                        r.addClass("stick");

                    } else {
                        r.removeClass("stick");

                    }
                });

            }


        }
        $(".list-group-item").click(function() {
            var target = jQuery(this).attr('href');
            $([document.documentElement, document.body]).animate({
                scrollTop: $(target).offset().top - 70
            }, 600);
            return false;
        });

        if (windowWidth >= 768) {
            jQuery('#item-sidebar,#menu-sidebar').theiaStickySidebar({
                additionalMarginTop: 30
            });

        }
        mybutton = document.getElementById("myBtn");
        wabutton = document.getElementById("whatsapp_button");

        // When the user scrolls down 20px from the top of the document, show the button
        window.onscroll = function() {
            if (mybutton && mybutton != null && mybutton != undefined) {
                scrollFunction();
            }
            if (wabutton && wabutton != null && wabutton != undefined) {
                scrollFunction();
            }
        };




    });

    function scrollFunction() {

        if ($(window).width() < 768 && (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20)) {
            if (mybutton != null && mybutton != undefined) {
                mybutton.style.display = "block";
            }

        } else {
            if (mybutton != null && mybutton != undefined) {
                mybutton.style.display = "none";
            }
        }

        if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
            if (wabutton != null && wabutton != undefined) {
                wabutton.style.display = "block";
            }

        } else if ($(window).width() > 768) {
            if (wabutton != null && wabutton != undefined) {
                wabutton.style.display = "block";
            }
        } else {
            if (wabutton != null && wabutton != undefined) {
                wabutton.style.display = "none";
            }
        }
    }

    // When the user clicks on the button, scroll to the top of the document
    function topFunction() {
        document.body.scrollTop = 0; // For Safari
        document.documentElement.scrollTop = 0; // For Chrome, Firefox, IE and Opera
    }

    function photo(input, name) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function(e) {
                input.style.width = '50%';
                $('#' + name + '').attr('src', e.target.result);
                $('#' + name + '').fadeIn();
            };
            reader.readAsDataURL(input.files[0]);
        }
    }
    //If user is logged in and loyalty program is enabled
    
    </script>





