<script>
	$(document).ready(function() {
		$('#cookiecountryint').change(function() {

			var country = $(this).val();


			if (country == '') {
				$('#cookieareaint').removeAttr("required");
				$('#cookieareaint').hide();
				$('#intshippingcheck').val('1')
				$('.local_shipping_fields').hide();
				$('.cookieareaintlabel').hide();
				$('.international_shipping_fields').show();
				$('.hidethisfield').hide();
				$('#intarea').val('').trigger('change');

			} else if (country == 'samecountry') {
				$('#cookieareaint').show();
				$('#intshippingcheck').val('0')
				$('#cookieareaint').val('');
				$('.cookieareaintlabel').show();
				$('#intarea').val('').trigger('change');
				$('#cookieareaint').attr("required", "required");
				$('.local_shipping_fields').show();
				$('.international_shipping_fields').hide();
				$('.hidethisfield').show();


			} else {
				$('#intshippingcheck').val('1')
				$('#cookieareaint').removeAttr("required");
				$('.cookieareaintlabel').hide();
				$('#cookieareaint').hide();
				$('.local_shipping_fields').hide();
				$('.international_shipping_fields').show();
				$('.hidethisfield').hide();
				$('#intarea').val(country).trigger('change');
			}
		});
		$('#cookieareaint').change(function() {

			var country = $(this).val();
			$('#intarea').val(country).trigger('change');

		});

		$("#couponForm").submit(function(event) {
			var cval = $("#promoCode").val();
			if (cval != '' && cval != null) {
				$("body").addClass("ajax-load");
				$.ajax({
					type: "POST",
					dataType: 'json',
					url: 'https://orderresto.orientalkuwait.com/welcome/add_coupon',
					data: 'ci_csrf_token=&code=' + cval,
					success: function(response) {
						$("body").removeClass("ajax-load");
						load_cart_div();
						load_cart_summary_div();

						if (response.message != '') {

							if (response.code == 404) {
								$('.coupon-alert').removeClass('alert-success');
								$('.coupon-alert').addClass('alert-danger');
							} else {
								$('.coupon-alert').removeClass('alert-danger');
								$('.coupon-alert').addClass('alert-success');
							}
							$('.coupon-alert').removeClass('hide');
							$('.coupon-alert').addClass('show');
							$('.coupon-message').html(response.message);
						} else {

							$('.coupon-alert').removeClass('show');
							$('.coupon-alert').addClass('hide');
							$('.coupon-message').html('');
						}
					}
				});
			}


			event.preventDefault();
		});
	});

	function get_adns_options(item_id) {

		if (item_id > 0) {

			$("#adons-options-block").empty();
			$('#addons-options-modal').modal('hide');

			$("body").addClass("ajax-load");
			$.ajax({
				url: 'https://orderresto.orientalkuwait.com/welcome/get_item_addons_options',
				type: 'POST',
				data: 'ci_csrf_token=&item_id=' + item_id,
				success: function(response) {

					$("body").removeClass("ajax-load");
					if (response != '') {
						$("#adons-options-block").html(response);

						$('#addons-options-modal').modal('show');
					} else {
						$("#adons-options-block").empty();
						$('#addons-options-modal').modal('hide');
					}
				}
			});

		}
	}

	function get_adns_options_dim(item_id) {

		if (item_id > 0) {

			$("#adons-options-block").empty();
			$('#addons-options-modal').modal('hide');

			$("body").addClass("ajax-load");
			$.ajax({
				url: 'https://orderresto.orientalkuwait.com/welcome/get_item_addons_options_dim',
				type: 'POST',
				data: 'ci_csrf_token=&item_id=' + item_id,
				success: function(response) {

					$("body").removeClass("ajax-load");
					if (response != '') {
						$("#adons-options-block").html(response);

						$('#addons-options-modal').modal('show');
					} else {
						$("#adons-options-block").empty();
						$('#addons-options-modal').modal('hide');
					}
				}
			});

		}
	}


	function get_cart_itm_adns_options(item_id, row_id) {


		if (row_id != "" && item_id > 0) {


			$("#adons-options-block").empty();
			$('#addons-options-modal').modal('hide');

			$("body").addClass("ajax-load");
			$.ajax({
				url: 'https://orderresto.orientalkuwait.com/welcome/get_cart_itm_adns_optns',
				type: 'POST',
				data: 'ci_csrf_token=&row_id=' + row_id + '&item_id=' + item_id,
				success: function(response) {

					$("body").removeClass("ajax-load");

					if (response != '') {

						$("#adons-options-block").html(response);
						update_cost();

						$("#addons-options-modal").modal('show');
					} else {
						$("#adons-options-block").empty();
						$("#addons-options-modal").modal('hide');
					}
				}
			});


		}
	}

	function load_cart_div() {

		$("#fc-cart-div").html('<img class="abs-loader" src="https://orderresto.orientalkuwait.com/assets/front/images/loader.gif" alt="Loader" align="middle">');

		$.ajax({
			url: 'https://orderresto.orientalkuwait.com/welcome/load_cart_div',
			type: 'POST',
			data: 'ci_csrf_token=',
			success: function(response) {


				if (response != '') {
					$("#fc-cart-div").html(response);
				} else {
					$("#fc-cart-div").empty();
				}
			}
		});
	}



	function load_cart_summary_div(otype = 'delivery') {

		$("body").addClass("ajax-load");

		$("#cart-div").html('<div class="loader-pad"><img class="abs-loader" src="https://orderresto.orientalkuwait.com/assets/front/images/loader.gif" alt="Loader" align="middle"></div>');

		$.ajax({
			url: 'https://orderresto.orientalkuwait.com/welcome/load_cart_summary_div/' + otype,
			type: 'POST',
			data: 'ci_csrf_token=',
			success: function(response) {

				$("body").removeClass("ajax-load");

				if (response != '') {
					$("#cart-div").html(response);
				} else {
					$("#cart-div").empty();
				}
			}
		});
	}


	function check_address(ua_id) {

		if (ua_id > 0) {


			$("#cart-div").html('<img class="abs-loader" src="https://orderresto.orientalkuwait.com/assets/front/images/loader.gif" alt="Loader" align="middle">');


			var checked = 'no';

			var discount = 0;

			discount = $("form#cart-form input[name='user_redeem_points']:checked").val();

			if (discount > 0)
				checked = 'yes';

			$("body").addClass("ajax-load");

			$.ajax({
				url: 'https://orderresto.orientalkuwait.com/welcome/load_cart_summary_div',
				type: 'POST',
				data: 'ci_csrf_token=&ua_id=' + ua_id + '&discount=' + discount + '&checked=' + checked,
				success: function(response) {

					$("body").removeClass("ajax-load");

					if (response != '') {
						$("#cart-div").html(response);
					} else {
						$("#cart-div").empty();
					}
				}
			});

		}
	}



	function get_item_popup(item_id, row_id) {

		if (item_id > 0 && row_id != "") {

			$("#itms-block").empty();
			$('#itm-modal').modal('hide');

			$("body").addClass("ajax-load");

			$.ajax({
				url: 'https://orderresto.orientalkuwait.com/welcome/get_item_popup',
				type: 'POST',
				data: 'ci_csrf_token=&row_id=' + row_id + '&item_id=' + item_id,
				success: function(response) {

					$("body").removeClass("ajax-load");

					if (response != '') {
						$("#itms-block").html(response);
						$('#itm-modal').modal('show');
					} else {
						$("#itms-block").empty();
						$('#itm-modal').modal('hide');
					}
				}
			});

		}
	}
</script>









<script type="text/javascript">
	(function($, W, D) {
		var JQUERY4U = {};

		JQUERY4U.UTIL = {
			setupFormValidation: function() {
				//Additional Methods

				//form validation rules
				$("#address_form").validate({
					ignore: "",
					rules: {
						city_id: {
							required: true
						},
						locality: {
							required: true
						},
						pincode: {
							required: true
						},
						house_no: {
							required: true
						},
						street: {
							required: true
						},
						landmark: {
							required: true
						}

					},
					messages: {
						city_id: {
							required: "City Required"
						},
						locality: {
							required: "Locality Required"
						},
						pincode: {
							required: "Pincode Required"
						},
						house_no: {
							required: "House Number Required"
						},
						street: {
							required: "Street Required"
						},
						landmark: {
							required: "Landmark Required"
						}

					},

					submitHandler: function(form) {
						form.submit();
					}
				});

			}
		}

		//when the dom has loaded setup form validation rules
		$(D).ready(function($) {
			JQUERY4U.UTIL.setupFormValidation();
		});

	})(jQuery, window, document);






	function get_localities() {

		$("#locality").empty();
		$("#locality").trigger('liszt:updated');

		$("#pincode").val('');

		var city_id = $("#city_id option:selected").val();

		if (city_id > 0) {

			$("body").addClass("ajax-load");

			$.ajax({
				type: 'POST',
				async: false,
				cache: false,
				url: "https://orderresto.orientalkuwait.com/cart/get_localities",
				data: 'ci_csrf_token=&city_id=' + city_id,
				success: function(data) {

					$("body").removeClass("ajax-load");

					if (data != '' && data != 0) {
						$('#locality').empty();
						$('#locality').append(data);
					} else if (data == 999) {
						window.location = 'https://orderresto.orientalkuwait.com/';
					}
					$("#locality").trigger("liszt:updated");
				}
			});
		}
	}


	function get_pincode() {

		$("#pincode").val('');

		var city_id = $("#city_id option:selected").val();
		var spl_id = $("#locality option:selected").val();

		if (city_id > 0 && spl_id > 0) {

			$("body").addClass("ajax-load");

			$.ajax({
				type: 'POST',
				async: false,
				cache: false,
				url: "https://orderresto.orientalkuwait.com/cart/get_pincode",
				data: 'ci_csrf_token=&city_id=' + city_id + '&spl_id=' + spl_id,
				success: function(data) {
					$("body").removeClass("ajax-load");

					if (data != '' && data != 0) {
						$('#pincode').val(data);
					} else if (data == 999) {
						window.location = 'https://orderresto.orientalkuwait.com/';
					}
				}
			});
		}
	}
</script>


