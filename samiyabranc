<script type="text/javascript">
	(function($, W, D) {
		var JQUERY4U = {};

		JQUERY4U.UTIL = {
			setupFormValidation: function() {
				//Additional Methods

				//form validation rules
				$("#login_form").validate({
					ignore: "",
					rules: {
						email: {
							required: true,
							email: true
						},
						password: {
							required: true
						}

					},
					messages: {
						email: {
							required: "Email Address Is Required",
							email: "Enter Valid Email Address"
						},
						password: {
							required: "Password Required"
						}
					},

					submitHandler: function(form) {
						form.submit();
					}
				});





				//form validation rules

				$("#forgot_password_form").validate({

					ignore: "",

					rules: {

						email: {
							required: true,
							email: true
						}
					},

					messages: {

						email: {

							required: "Email Address Is Required",
							email: "Enter Valid Email Address"

						}


					},



					submitHandler: function(form) {

						form.submit();

					}

				});

			}
		}

		//when the dom has loaded setup form validation rules
		$(D).ready(function($) {
			JQUERY4U.UTIL.setupFormValidation();
		});

	})(jQuery, window, document);
</script>


<script type="text/javascript">
	// A $( document ).ready() block.
	$(document).ready(function() {


		$.ajaxPrefilter(function(options, originalOptions, jqXHR) {
			options.async = true;
		});

		
		$('.takeawayswitch').click(function() {
			$('.deliverydiv').hide();
			$('.takeawayswitch').addClass("active");
			$('.deliverybtnswith').removeClass("active");
			$('.pickupdiv').show();

		});

		$('.deliverybtnswith').click(function() {
			$('.pickupdiv').hide();
			$('.takeawayswitch').removeClass("active");
			$('.deliverybtnswith').addClass("active");
			$('.deliverydiv').show();
		});

			});



	function show_popup(type) {

		

		$("#signup-modal").modal('hide');
		$("#forgot-pwd-modal").modal('hide');

		if (type == 'signup') {
			$("#signup-modal").modal('show');
		} else if (type == 'forgot_pwd') {
			$("#forgot-pwd-modal").modal('show');
		}
	}
</script>

